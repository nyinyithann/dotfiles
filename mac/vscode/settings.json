      "before": ["<"]
    }
  ],
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "liveServer.settings.proxy": {
    "enable": true,
    "baseUri": "/fetch",
    "proxyUri": "http://localhost:5000/api"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "python.insidersChannel": "daily",
  "editor.codeLensFontSize": 12,
  "vim.joinspaces": true,
  "vim.useSystemClipboard": true,
  "terminal.integrated.fontSize": 12,
  "files.autoSave": "afterDelay",
  "liveServer.settings.port": 5010,
  "workbench.iconTheme": "material-icon-theme",
  "vim.showcmd": true,
  "material-icon-theme.saturation": 1,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.fontFamily": "'FiraMono Nerd Font', 'YaHei Consolas Hybrid', 'Ubuntu Mono', 'Noto Mono', 'DejaVu Sans Mono'",
  "[css]": {
    "editor.defaultFormatter": "Wscats.qf"
  },
  "material-icon-theme.folders.color": "#42a5f5",
  "editor.accessibilitySupport": "off",
  "jest.autoRun": "false",
  "oneDarkPro.italic": false,
  "editor.mouseWheelZoom": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "emmet.excludeLanguages": ["markdown"],
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "[ocaml]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "ocamllabs.ocaml-platform",
    "editor.formatOnSave": true,
    "editor.insertSpaces": true
  },
  "liveServer.settings.host": "localhost",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.hlsearch": true,
  "material-icon-theme.opacity": 0.69999999999999996,
  "eslint.validate": ["javascript"],
  "terminal.explorerKind": "external",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.cursorStyle": "line",
  "redhat.telemetry.enabled": true,
  "workbench.sideBar.location": "left",
  "FSharp.useSdkScripts": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll": true
  },
  "terminal.integrated.fontWeight": "600",
  "debug.console.fontSize": 146,
  "explorer.confirmDelete": false,
  "remote.WSL.debug": true,
  "terminal.integrated.cursorBlinking": true,
  "vim.handleKeys": {
    "<C-f>": false,
    "<C-a>": false,
    "<C-v>": false,
    "<C-d>": true,
    "<C-y>": false,
    "<C-z>": false,
    "<C-c>": false,
    "<C-x>": false
  },
  "svelte.enable-ts-plugin": true,
  "vim.autoindent": true,
  "vim.ignorecase": true,
  "scss.lint.unknownAtRules": "ignore",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": ["d", "d"],
      "before": ["<leader>", "d"]
    },
    {
      "commands": [":nohl"],
      "before": ["<C-n>"]
    },
    {
      "commands": ["workbench.action.nextEditor"],
      "before": ["<tab>"]
    },
    {
      "commands": ["workbench.action.previousEditor"],
      "before": ["<S-tab>"]
    },
    {
      "commands": ["workbench.action.toggleSidebarVisibility"],
      "before": ["<leader>", "e"]
    },
    {
      "commands": ["revealInExplorer"],
      "before": ["<leader>", "f"]
    },
    {
      "commands": ["workbench.view.explorer"],
      "before": ["<leader>", "h"]
    },
    {
      "commands": ["workbench.action.showCommands"],
      "before": [":"]
    },
    {
      "commands": ["workbench.action.files.save"],
      "before": ["<C-s>"]
    },
    {
      "before": ["K"],
      "commands": [{ "command": "editor.action.showDefinitionPreviewHover" }]
    }
  ],
  "[dart]": {
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.selectionHighlight": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80]
  },
  "prettier.singleQuote": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "k"]
    }
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font Mono','CaskaydiaCove Nerd Font','MesloLGS NF', 'Cascadia Mono PL'",
  "diffEditor.ignoreTrimWhitespace": false,
  "scss.validate": false,
  "editor.lineHeight": 24,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.fontSize": 13,
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "line",
  "Lua.telemetry.enable": true,

  "[csharp]": {
    "editor.formatOnSave": true
  },
  "editor.formatOnType": true,
  "[sql]": {
    "editor.defaultFormatter": "ms-ossdata.vscode-postgresql"
  },
  "workbench.colorTheme": "Ambiance [beta]"
}
